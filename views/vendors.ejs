<% include ./partials/header %>

<div class="contain text-center m-0 pr-5">
    <div class="row">
        <div class="col-md-2 text-left position-fixed upper">
            <a class="btn bg-black my-2 w-75" href="/venNet/new"><i class="fas fa-angle-up p-2 float-left"></i> Add
                Individual</a>
            <a class="btn bg-black my-2 w-75" href="/venNet/upload"><i
                    class="fas fa-angle-double-up p-2 float-left"></i> Add Multiple</a>
            <a class="btn bg-black my-2 w-75" href="/venNet/download"><i class="fas fa-file-alt p-2 float-left"></i>
                Download</a>
        </div>
        <div class="col-md-2">

        </div>
        <div class="col-md-10 translucentBox p-0 pb-4">
            <h1 class="text-center bg-black">Vendor Network</h1>
            <div class="row pt-4">
                <div class="col-md-2"></div>
                <div class="form-row col-md-8 text-center">
                    <div class="form-group col-md-11">
                        <input id="vendorSearch" type="text" class="form-control" name="Search" autofocus>
                    </div>
                    <div class="form-group col-md-1 ">
                        <a onclick="vendorSearch()" class="p-4">
                            <i class="fas fa-search"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="pr-5">Search Type </label>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="searchBy" id="completedRadio1" value="id">
                    <label class="form-check-label" for="completedRadio1">Id</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="searchBy" id="completedRadio2" value="city"
                        checked>
                    <label class="form-check-label" for="completedRadio2">City</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="searchBy" id="completedRadio3"
                        value="vendorName">
                    <label class="form-check-label" for="completedRadio3">Vendor Name</label>
                </div>
            </div>
            <table align="center" class="tab" id="append">

            </table>
        </div>
    </div>
</div>

<% include ./partials/footer2 %>